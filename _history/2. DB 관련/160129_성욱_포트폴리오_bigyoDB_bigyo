-- 테이블 bigyo.Manage_Board 구조 내보내기
CREATE TABLE IF NOT EXISTS `Manage_Board` (
  `Board_ID` int(11) NOT NULL COMMENT '고유번호',
  `Board_Name` varchar(30) NOT NULL COMMENT '게시판 이름',
  `Board_Title` varchar(30) NOT NULL COMMENT '게시판 타이틀',
  `Categories` text NOT NULL COMMENT '카테고리',
  `Page_Per_Record` int(4) NOT NULL COMMENT '페이지당 게시물 숫자',
  `Block_Per_Page` int(4) NOT NULL COMMENT '블락당 페이지 숫자',
  `Attachment_Num` int(4) unsigned NOT NULL COMMENT '첨부파일 숫자',
  `Hot_Count` int(4) NOT NULL COMMENT 'HOT 게시물 조회수',
  `Hot_Comment` int(4) NOT NULL COMMENT 'HOT 코멘트 수',
  `Hot_Like` int(4) NOT NULL COMMENT 'HOT 좋아요 수',
  `Hot_Dislike` int(4) NOT NULL COMMENT 'HOT 싫어요 수',
  `Post_Point` int(4) NOT NULL COMMENT '게시물 작성 포인트',
  `Read_Point` int(4) NOT NULL COMMENT '게시물 조회 포인트',
  `Comment_Point` int(4) NOT NULL COMMENT '댓글 작성 포인트',
  `Selected_Point` int(4) NOT NULL COMMENT '답변 채택 포인트',
  `Download_Point` int(4) NOT NULL COMMENT '다운로드 포인트',
  `Vote_Point` int(4) NOT NULL COMMENT '투표 포인트',
  `Like_Point` int(4) NOT NULL COMMENT '좋아요 포인트',
  `Dislike_Point` int(4) NOT NULL COMMENT '싫어요 포인트',
  `View_Level` int(2) NOT NULL COMMENT '게시판 목록 읽기 권한',
  `Post_Level` int(2) NOT NULL COMMENT '게시판 쓰기 권한',
  `Read_Level` int(2) NOT NULL COMMENT '게시판 읽기 권한',
  `Search_Level` int(2) NOT NULL COMMENT '검색 권한',
  `Comment_Level` int(2) NOT NULL COMMENT '코멘트 권한',
  `Attachment_upload_level` int(2) NOT NULL COMMENT '파일 업로드 권한',
  `Attachment_download_level` int(2) NOT NULL COMMENT '파일 다운로드 권한',
  `Like_Level` int(2) NOT NULL COMMENT '좋아요/싫어요 권한',
  `Vote_Level` int(2) NOT NULL COMMENT '투표 권한',
  `IP_View_Level` int(2) NOT NULL COMMENT 'IP 보이기 권한',
  `Notice_Level` int(2) NOT NULL COMMENT '공지 권한',
  PRIMARY KEY  (`Board_ID`),
  UNIQUE KEY `Board_Name` (`Board_Name`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COMMENT='각 게시판을 관리하는 테이블입니다.';

