-- 테이블 bigyo.Members 구조 내보내기
CREATE TABLE IF NOT EXISTS `Members` (
  `Num` int(11) NOT NULL auto_increment COMMENT '회원번호',
  `ID` varchar(20) default NULL COMMENT '아이디',
  `Facebook` varchar(40) default NULL COMMENT '페이스북',
  `Twitter` varchar(40) default NULL COMMENT '트위터',
  `Name` varchar(50) NOT NULL COMMENT '별명',
  `Pass` varchar(2000) NOT NULL COMMENT '비밀번호',
  `Email` varchar(100) NOT NULL default ' ' COMMENT '이메일',
  `About` varchar(500) default ' ' COMMENT '자기소개',
  `Signature` text COMMENT '서명',
  `Gender` varchar(140) default 'male' COMMENT '성별',
  `Birth` int(4) default '1990' COMMENT '출생년도',
  `Point` int(11) default '0' COMMENT '포인트',
  `Level` int(2) default '1' COMMENT '등급',
  `Picture` varchar(100) default '/images/members/basic.jpg' COMMENT '사진',
  `Interest` varchar(100) default NULL COMMENT '관심사항',
  `Message` varchar(10) NOT NULL default '전체 수락' COMMENT '쪽지수신범위',
  `Join_Date` datetime default NULL COMMENT '가입날짜',
  `Last_Login_Date` datetime default NULL COMMENT '최근접속',
  `Prison_Break` datetime default NULL COMMENT '마지막징계',
  PRIMARY KEY  (`Num`),
  UNIQUE KEY `Name` (`Name`),
  UNIQUE KEY `ID` (`ID`),
  UNIQUE KEY `Facebook_Twitter` (`Facebook`,`Twitter`)
) ENGINE=MyISAM AUTO_INCREMENT=18 DEFAULT CHARSET=utf8 COMMENT='회원정보 테이블입니다.';

