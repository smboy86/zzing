-- 테이블 bigyo.Sample_Board 구조 내보내기
CREATE TABLE IF NOT EXISTS `Sample_Board` (
  `Post_ID` int(11) NOT NULL auto_increment COMMENT '글번호',
  `Group_ID` int(11) default NULL COMMENT '글그룹',
  `Member_Num` int(11) default NULL COMMENT '회원번호',
  `Member_Name` varchar(20) default NULL COMMENT '별명',
  `Category` varchar(20) default NULL COMMENT '카테고리',
  `Subject` varchar(50) default NULL COMMENT '제목',
  `Content` varchar(5000) default NULL COMMENT '내용',
  `Count` int(11) NOT NULL default '0' COMMENT '카운터',
  `IP` varchar(50) default NULL COMMENT 'IP',
  `Comment_Group` int(11) NOT NULL default '0' COMMENT '댓글그룹',
  `ComCom_Code` int(11) default '0' COMMENT '대댓글 코드',
  `Comment_Count` int(11) default '0' COMMENT '댓글 갯수',
  `Like` int(11) NOT NULL default '0' COMMENT '좋아요 카운트',
  `Dislike` int(11) NOT NULL default '0' COMMENT '싫어요 카운트',
  `Attachment` int(11) NOT NULL default '0' COMMENT '첨부파일',
  `Modify_Count` int(11) NOT NULL default '0' COMMENT '수정횟수',
  `Select` varchar(50) NOT NULL default '0' COMMENT '채택답변',
  `Post_Date` datetime default NULL COMMENT '작성일자',
  `Delete_Date` datetime default NULL COMMENT '삭제일자',
  `Read_Date` datetime default NULL COMMENT '마지막 읽은 시간',
  PRIMARY KEY  (`Post_ID`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COMMENT='댓글번호 0은 본문\r\n첫댓글은 1, 두번째 댓글은 2...\r\n1의 대댓글은 1 + 대댓코드 A';

-- 테이블 데이터 bigyo.Sample_Board:0 rows 내보내기
/*!40000 ALTER TABLE `Sample_Board` DISABLE KEYS */;
/*!40000 ALTER TABLE `Sample_Board` ENABLE KEYS */;


-- 테이블 bigyo.Sample_Board_Attachment 구조 내보내기
CREATE TABLE IF NOT EXISTS `Sample_Board_Attachment` (
  `Attach_ID` int(11) NOT NULL auto_increment COMMENT '첨부번호',
  `Post_ID` int(11) default NULL COMMENT '글번호',
  `Member_Num` int(11) default NULL COMMENT '회원번호',
  `File_Name` varchar(1000) default NULL COMMENT '퍄앨이름',
  `Download_Count` int(11) default NULL COMMENT '다운받은 횟수',
  `Upload_Date` datetime default NULL COMMENT '업로드한 날짜',
  `Img_Width` int(11) default NULL COMMENT '이미지 가로길이',
  `Img_Height` int(11) default NULL COMMENT '이미지 세로길이',
  PRIMARY KEY  (`Attach_ID`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COMMENT='첨부파일';

