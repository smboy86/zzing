-- 테이블 bigyo.Board_Forum_Attachment 구조 내보내기
CREATE TABLE IF NOT EXISTS `Board_Forum_Attachment` (
  `Attach_ID` int(11) NOT NULL auto_increment COMMENT '첨부번호',
  `Post_ID` int(11) NOT NULL COMMENT '글번호',
  `Member_Num` int(11) default NULL COMMENT '회원번호',
  `File_Name` varchar(200) NOT NULL COMMENT '파일이름',
  `Original_Name` varchar(200) default NULL COMMENT '원래 파일이름',
  `Content_type` varchar(50) default NULL COMMENT '파일 형식',
  `File_Size` int(11) NOT NULL COMMENT '파일 용량',
  `Download_Count` int(11) NOT NULL COMMENT '다운받은 횟수',
  `Upload_Date` datetime default NULL COMMENT '업로드한 날짜',
  `Delete_Date` datetime default NULL COMMENT '삭제한 날짜',
  PRIMARY KEY  (`Attach_ID`)
) ENGINE=MyISAM AUTO_INCREMENT=67 DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC COMMENT='첨부파일';

