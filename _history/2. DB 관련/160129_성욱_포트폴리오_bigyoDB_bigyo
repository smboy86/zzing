-- 테이블 bigyo.Board_Market 구조 내보내기
CREATE TABLE IF NOT EXISTS `Board_Market` (
  `Post_ID` int(11) NOT NULL auto_increment COMMENT '글번호',
  `Member_Num` int(11) NOT NULL COMMENT '회원번호',
  `Member_Name` varchar(20) NOT NULL COMMENT '별명',
  `Category` varchar(50) NOT NULL COMMENT '카테고리',
  `Subject` varchar(50) NOT NULL COMMENT '제목',
  `Content` varchar(5000) NOT NULL COMMENT '내용',
  `Count` int(11) NOT NULL default '0' COMMENT '카운터',
  `IP` varchar(50) default NULL COMMENT 'IP',
  `Comment_Count` int(11) NOT NULL default '0' COMMENT '댓글갯수',
  `Comment_Num` int(11) NOT NULL default '0' COMMENT '댓글번호',
  `ComCom_Code` varchar(10) default NULL COMMENT '대댓글 코드',
  `Attachment` int(11) NOT NULL default '0' COMMENT '첨부파일',
  `Post_Date` datetime default NULL COMMENT '작성일자',
  `Delete_Date` datetime default NULL COMMENT '삭제일자',
  `Price` varchar(50) default NULL COMMENT '가격',
  `Contact` varchar(50) default NULL COMMENT '연락처',
  `Method` varchar(50) default NULL COMMENT '거래방법',
  `Sold_out` int(1) default '0' COMMENT '거래완료',
  PRIMARY KEY  (`Post_ID`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC COMMENT='거래중0\r\n거래종료1\r\n\r\n';

