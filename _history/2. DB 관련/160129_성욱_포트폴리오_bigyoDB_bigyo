/*!40101 SET @OLD_CHARACTER_SET_CLIENT=@@CHARACTER_SET_CLIENT */;
/*!40101 SET NAMES utf8 */;
/*!40014 SET @OLD_FOREIGN_KEY_CHECKS=@@FOREIGN_KEY_CHECKS, FOREIGN_KEY_CHECKS=0 */;
/*!40101 SET @OLD_SQL_MODE=@@SQL_MODE, SQL_MODE='NO_AUTO_VALUE_ON_ZERO' */;

-- bigyo 데이터베이스 구조 내보내기
CREATE DATABASE IF NOT EXISTS `bigyo` /*!40100 DEFAULT CHARACTER SET utf8 */;
USE `bigyo`;


-- 테이블 bigyo.Board_Forum 구조 내보내기
CREATE TABLE IF NOT EXISTS `Board_Forum` (
  `Post_ID` int(11) NOT NULL auto_increment COMMENT '글번호',
  `Part_ID` int(11) NOT NULL default '0' COMMENT '게시판 파트',
  `Notice` int(1) NOT NULL default '0' COMMENT '공지사항',
  `Group_ID` int(11) NOT NULL default '0' COMMENT '글그룹',
  `Member_Num` int(11) default NULL COMMENT '회원번호',
  `Member_Name` varchar(30) default NULL COMMENT '별명',
  `Subject` varchar(50) default NULL COMMENT '제목',
  `Content` text NOT NULL COMMENT '내용',
  `Category` varchar(20) default NULL COMMENT '카테고리',
  `Count` int(11) NOT NULL default '0' COMMENT '카운터',
  `IP` varchar(50) default NULL COMMENT 'IP',
  `Reply_Group` int(11) NOT NULL default '0' COMMENT '답글 그룹',
  `Reply_Seq` int(11) NOT NULL default '0' COMMENT '답글 순서',
  `Reply_Level` int(11) NOT NULL default '0' COMMENT '답글 레벨',
  `Comment_Seq` int(11) NOT NULL default '0' COMMENT '댓글 순서',
  `Comm_Reply_Seq` int(11) NOT NULL default '0' COMMENT '대댓글 순서',
  `Comment_Count` int(11) NOT NULL default '0' COMMENT '댓글 갯수',
  `Likes` int(11) NOT NULL default '0' COMMENT '좋아요 카운트',
  `Dislike` int(11) NOT NULL default '0' COMMENT '싫어요 카운트',
  `Like_List` text COMMENT '좋아요/싫어요 누른 사람들',
  `Attachment` int(2) NOT NULL default '0' COMMENT '첨부파일',
  `Modify_Count` int(2) NOT NULL default '0' COMMENT '수정횟수',
  `Post_Date` datetime NOT NULL COMMENT '작성일자',
  `Delete_Date` datetime default NULL COMMENT '삭제일자',
  `Read_Date` datetime default NULL COMMENT '마지막 읽은 시간',
  `Method` varchar(50) default NULL,
  `Contact` varchar(50) default NULL,
  `Price` varchar(50) default NULL,
  `Selected` varchar(50) default NULL,
  PRIMARY KEY  (`Post_ID`)
) ENGINE=MyISAM AUTO_INCREMENT=108 DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC COMMENT='댓글번호 0은 본문\r\n첫댓글은 1, 두번째 댓글은 2...\r\n1의 대댓글은 1 + 대댓코드 A';

