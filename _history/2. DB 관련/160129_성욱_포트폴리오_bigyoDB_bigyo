-- 테이블 bigyo.Message_Save 구조 내보내기
CREATE TABLE IF NOT EXISTS `Message_Save` (
  `Num` int(11) NOT NULL auto_increment COMMENT '쪽지번호',
  `Send_Member` int(11) default NULL COMMENT '보낸사람',
  `Receive_Member` int(11) default NULL COMMENT '받는사람',
  `Send_Date` datetime default NULL COMMENT '보낸시간',
  `Read_Date` datetime default NULL COMMENT '읽은시간',
