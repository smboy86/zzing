-- 테이블 bigyo.Like_Avg 구조 내보내기
CREATE TABLE IF NOT EXISTS `Like_Avg` (
  `Post_ID` int(11) NOT NULL,
  `Likes` int(11) unsigned NOT NULL default '0',
  `Dislike` int(11) unsigned NOT NULL default '0',
  `Male_Like` int(11) unsigned NOT NULL default '0',
  `Male_Dislike` int(11) unsigned NOT NULL default '0',
  `Female_Like` int(11) unsigned NOT NULL default '0',
  `Female_Dislike` int(11) unsigned NOT NULL default '0',
  `Age_Like` int(11) unsigned NOT NULL default '0',
  `Age_Dislike` int(11) unsigned NOT NULL default '0',
  `Like_List` text,
  PRIMARY KEY  (`Post_ID`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;

