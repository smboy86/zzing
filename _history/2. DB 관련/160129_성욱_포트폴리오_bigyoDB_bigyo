-- 프로시저 bigyo.sp_board_attach_delete 구조 내보내기
DELIMITER //
CREATE DEFINER=`bigyo`@`%` PROCEDURE `sp_board_attach_delete`(
		IN pi_attach_id INT,
		IN pi_post_id INT)
BEGIN
	update Board_Forum_Attachment set delete_date = now() where attach_id = pi_attach_id;
	update Board_Forum set attachment = attachment -1 where post_id = pi_post_id;
END//
DELIMITER ;


-- 프로시저 bigyo.sp_board_comment 구조 내보내기
DELIMITER //
CREATE DEFINER=`bigyo`@`%` PROCEDURE `sp_board_comment`(
		IN pi_part_id INT,
		IN pi_member_num INT,
		IN pi_member_name VARCHAR(30),
		IN pi_content TEXT,
		IN pi_ip VARCHAR(50),
		IN pi_group_id INT,
		IN pi_comment_seq INT,
		IN pi_parent_post_id INT,
		IN pi_parent_comment_id INT)
BEGIN
	DECLARE v_comment_point INT;
	select comment_point into v_comment_point from Manage_Board where board_id = pi_part_id;
	update Members set point = point + v_comment_point where num = pi_member_num;
	update Board_Forum set comment_count = comment_count+1 where post_id = pi_parent_post_id;

	IF pi_parent_comment_id = 0 OR pi_parent_comment_id is null THEN
		insert into
			Board_Forum(part_id, member_num, member_name, content, ip, post_date, group_id, comment_seq)
			select
				pi_part_id, pi_member_num, pi_member_name, pi_content, pi_ip, now(), pi_group_id,
				max(comment_seq)+1 from Board_Forum where group_id = pi_group_id;

	ELSEIF pi_comment_seq > 0 THEN
		update Board_Forum set comment_count = comment_count+1 where post_id = pi_parent_comment_id;
		insert into
			Board_Forum(part_id,member_num, member_name, content, ip, post_date, group_id, comment_seq, comm_reply_seq)
			select
				pi_part_id, pi_member_num, pi_member_name, pi_content, pi_ip, now(), pi_group_id,
				pi_comment_seq, max(comm_reply_seq)+1 from Board_Forum
				where group_id = pi_group_id and comment_seq = pi_comment_seq;
	END IF;
END//
DELIMITER ;


-- 프로시저 bigyo.sp_board_comment_delete 구조 내보내기
DELIMITER //
CREATE DEFINER=`bigyo`@`%` PROCEDURE `sp_board_comment_delete`(
		IN pi_post_id INT,
		IN pi_parent_post_id INT,
		IN pi_parent_comment_id INT,
		IN pi_member_num INT)
BEGIN
	DECLARE v_comment_point INT;
	DECLARE v_part_id INT;
	select part_id into v_part_id from Board_Forum where post_id = pi_parent_post_id;
	select comment_point into v_comment_point from Manage_Board where board_id = v_part_id;
	update Members set point = point - v_comment_point where num = pi_member_num;
	update Board_Forum set comment_count = comment_count-1 where post_id = pi_parent_post_id;

	IF pi_parent_comment_id = 0 OR pi_parent_comment_id is null THEN
		update Board_Forum set delete_date = now() where post_id = pi_post_id;

	ELSEIF pi_comment_seq > 0 THEN
		update Board_Forum set comment_count = comment_count-1 where post_id = pi_parent_comment_id;
		update Board_Forum set delete_date = now() where post_id = pi_post_id;
	END IF;
END//
DELIMITER ;


-- 프로시저 bigyo.sp_board_delete 구조 내보내기
DELIMITER //
CREATE DEFINER=`bigyo`@`%` PROCEDURE `sp_board_delete`(
		IN pi_post_id INT,
		IN pi_member_num INT)
BEGIN
	DECLARE v_post_point INT;
	DECLARE v_part_id INT;
	select part_id into v_part_id from Board_Forum where post_id = pi_post_id;
	select post_point into v_post_point from Manage_Board where board_id = v_part_id;
	update Members set point = point - v_post_point where num = pi_member_num;
	update Board_Forum set delete_date = now() where post_id = pi_post_id;
END//
DELIMITER ;


-- 프로시저 bigyo.sp_board_list 구조 내보내기
DELIMITER //
CREATE DEFINER=`bigyo`@`%` PROCEDURE `sp_board_list`(
		IN pi_part_id INT,
		IN pi_page_id INT,
		IN pi_category VARCHAR(20),
		IN pi_select VARCHAR(20),
		IN pi_search VARCHAR(100))
BEGIN
	DECLARE v_limit INT;
	select page_per_record into v_limit from Manage_Board where board_id = pi_part_id;

	SET @part_id = pi_part_id;
	SET @limit_id = v_limit;
	SET @category = pi_category;
	SET @select = pi_select;
	SET @search = pi_search;
	SET @offset_id = pi_page_id*v_limit;
	
	IF pi_category is null AND pi_search is null THEN
		SET @query = "select *, 
			if(DATE_FORMAT(now(), '%Y-%m-%d')=DATE_FORMAT(post_date, '%Y-%m-%d'), 
				DATE_FORMAT(post_date, '%H시 %i분'), DATE_FORMAT(post_date, '%y-%m-%d')) as post_time
			from Board_Forum where part_id =? and subject is not null and delete_date is null
			order by notice desc, reply_group desc, reply_seq asc, group_id desc, post_id desc
			limit ? offset ?";
		PREPARE stmt FROM @query;
		EXECUTE stmt USING @part_id, @limit_id, @offset_id;

	ELSEIF pi_category is not null AND pi_search is null THEN
		SET @query = "select *, 
			if(DATE_FORMAT(now(), '%Y-%m-%d')=DATE_FORMAT(post_date, '%Y-%m-%d'), 
				DATE_FORMAT(post_date, '%H시 %i분'), DATE_FORMAT(post_date, '%y-%m-%d')) as post_time from
			Board_Forum where part_id =? and category =? 
			and subject is not null and delete_date is null order by notice desc, reply_group desc, reply_seq asc, group_id desc, post_id desc
			limit ? offset ?";
		PREPARE stmt FROM @query;
		EXECUTE stmt USING @part_id, @category, @limit_id, @offset_id;

	ELSEIF pi_search is not null THEN
		IF pi_select = 'subject' THEN
			SET @query = "select *, 
				if(DATE_FORMAT(now(), '%Y-%m-%d')=DATE_FORMAT(post_date, '%Y-%m-%d'), 
					DATE_FORMAT(post_date, '%H시 %i분'), DATE_FORMAT(post_date, '%y-%m-%d')) as post_time from
				Board_Forum where part_id =? and subject like concat('%',?,'%')
				and subject is not null and delete_date is null order by notice desc, reply_group desc, reply_seq asc, group_id desc, post_id desc
				limit ? offset ?";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @search, @limit_id, @offset_id;

		ELSEIF pi_select = 'content' THEN
			SET @query = "select *, 
				if(DATE_FORMAT(now(), '%Y-%m-%d')=DATE_FORMAT(post_date, '%Y-%m-%d'), 
					DATE_FORMAT(post_date, '%H시 %i분'), DATE_FORMAT(post_date, '%y-%m-%d')) as post_time from
				Board_Forum where part_id =? and content like concat('%',?,'%')
				and subject is not null and delete_date is null order by notice desc, reply_group desc, reply_seq asc, group_id desc, post_id desc
				limit ? offset ?";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @search, @limit_id, @offset_id;

		ELSEIF pi_select = 'comment' THEN
			SET @query = "select *, 
				if(DATE_FORMAT(now(), '%Y-%m-%d')=DATE_FORMAT(post_date, '%Y-%m-%d'), 
					DATE_FORMAT(post_date, '%H시 %i분'), DATE_FORMAT(post_date, '%y-%m-%d')) as post_time from
				Board_Forum where part_id =? and subject is not null and group_id in
				(select group_id from
					Board_Forum where part_id=? content like concat('%',?,'%')
					and comment_seq > 0 and delete_date is null)
				order by notice desc, reply_group desc, reply_seq asc, group_id desc, post_id desc
				limit ? offset ?";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @part_id, @search, @limit_id, @offset_id;

		ELSEIF pi_select = 'subject_content' THEN
			SET @query = "select *, 
				if(DATE_FORMAT(now(), '%Y-%m-%d')=DATE_FORMAT(post_date, '%Y-%m-%d'), 
					DATE_FORMAT(post_date, '%H시 %i분'), DATE_FORMAT(post_date, '%y-%m-%d')) as post_time from
				Board_Forum where part_id =? and
				(subject like concat('%',?,'%') or content like concat('%',?,'%'))
				and subject is not null and delete_date is null order by notice desc, reply_group desc, reply_seq asc, group_id desc, post_id desc
				limit ? offset ?";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @search, @search, @limit_id, @offset_id;

		ELSEIF pi_select = 'member_name' THEN
			SET @query = "select *, 
				if(DATE_FORMAT(now(), '%Y-%m-%d')=DATE_FORMAT(post_date, '%Y-%m-%d'), 
					DATE_FORMAT(post_date, '%H시 %i분'), DATE_FORMAT(post_date, '%y-%m-%d')) as post_time from
				Board_Forum where part_id =? and member_name = ?
				and subject is not null and delete_date is null order by notice desc, reply_group desc, reply_seq asc, group_id desc, post_id desc
				limit ? offset ?";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @search, @limit_id, @offset_id;

		ELSEIF pi_select = 'member_name_comment' THEN
			SET @query = "select *, 
				if(DATE_FORMAT(now(), '%Y-%m-%d')=DATE_FORMAT(post_date, '%Y-%m-%d'), 
					DATE_FORMAT(post_date, '%H시 %i분'), DATE_FORMAT(post_date, '%y-%m-%d')) as post_time from
				Board_Forum where part_id =? and subject is not null and group_id in
				(select group_id from
					Board_Forum where part_id =? and member_name = ?
					and comment_seq > 0 and delete_date is null)
				limit ? offset ?";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @part_id, @search, @limit_id, @offset_id;
		END IF;
	END IF;
END//
DELIMITER ;


-- 프로시저 bigyo.sp_board_list_del 구조 내보내기
DELIMITER //
CREATE DEFINER=`bigyo`@`%` PROCEDURE `sp_board_list_del`(
		IN pi_part_id INT,
		IN pi_page_id INT,
		IN pi_category VARCHAR(20),
		IN pi_select VARCHAR(20),
		IN pi_search VARCHAR(100))
BEGIN
	DECLARE v_limit INT;
	select page_per_record into v_limit from Manage_Board where board_id = pi_part_id;

	SET @part_id = pi_part_id;
	SET @limit_id = v_limit;
	SET @category = pi_category;
	SET @select = pi_select;
	SET @search = pi_search;
	SET @offset_id = pi_page_id*v_limit;
	
	IF pi_category is null AND pi_search is null THEN
		SET @query = "select *, 
			if(DATE_FORMAT(now(), '%Y-%m-%d')=DATE_FORMAT(delete_date, '%Y-%m-%d'), 
				DATE_FORMAT(delete_date, '%H시 %i분'), DATE_FORMAT(delete_date, '%y-%m-%d')) as post_time
			from Board_Forum where part_id =? and subject is not null and delete_date is not null
			order by notice desc, reply_group desc, reply_seq asc, group_id desc, post_id desc
			limit ? offset ?";
		PREPARE stmt FROM @query;
		EXECUTE stmt USING @part_id, @limit_id, @offset_id;

	ELSEIF pi_category is not null AND pi_search is null THEN
		SET @query = "select *, 
			if(DATE_FORMAT(now(), '%Y-%m-%d')=DATE_FORMAT(delete_date, '%Y-%m-%d'), 
				DATE_FORMAT(delete_date, '%H시 %i분'), DATE_FORMAT(delete_date, '%y-%m-%d')) as post_time from
			Board_Forum where part_id =? and category =? 
			and subject is not null and delete_date is not null order by notice desc, reply_group desc, reply_seq asc, group_id desc, post_id desc
			limit ? offset ?";
		PREPARE stmt FROM @query;
		EXECUTE stmt USING @part_id, @category, @limit_id, @offset_id;

	ELSEIF pi_search is not null THEN
		IF pi_select = 'subject' THEN
			SET @query = "select *, 
				if(DATE_FORMAT(now(), '%Y-%m-%d')=DATE_FORMAT(delete_date, '%Y-%m-%d'), 
					DATE_FORMAT(delete_date, '%H시 %i분'), DATE_FORMAT(delete_date, '%y-%m-%d')) as post_time from
				Board_Forum where part_id =? and subject like concat('%',?,'%')
				and subject is not null and delete_date is not null order by notice desc, reply_group desc, reply_seq asc, group_id desc, post_id desc
				limit ? offset ?";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @search, @limit_id, @offset_id;

		ELSEIF pi_select = 'content' THEN
			SET @query = "select *, 
				if(DATE_FORMAT(now(), '%Y-%m-%d')=DATE_FORMAT(delete_date, '%Y-%m-%d'), 
					DATE_FORMAT(delete_date, '%H시 %i분'), DATE_FORMAT(delete_date, '%y-%m-%d')) as post_time from
				Board_Forum where part_id =? and content like concat('%',?,'%')
				and subject is not null and delete_date is not null order by notice desc, reply_group desc, reply_seq asc, group_id desc, post_id desc
				limit ? offset ?";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @search, @limit_id, @offset_id;

		ELSEIF pi_select = 'comment' THEN
			SET @query = "select *, 
				if(DATE_FORMAT(now(), '%Y-%m-%d')=DATE_FORMAT(delete_date, '%Y-%m-%d'), 
					DATE_FORMAT(delete_date, '%H시 %i분'), DATE_FORMAT(delete_date, '%y-%m-%d')) as post_time from
				Board_Forum where part_id =? and subject is not null and group_id in
				(select group_id from
					Board_Forum where part_id=? content like concat('%',?,'%')
					and comment_seq > 0 and delete_date is not null)
				order by notice desc, reply_group desc, reply_seq asc, group_id desc, post_id desc
				limit ? offset ?";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @part_id, @search, @limit_id, @offset_id;

		ELSEIF pi_select = 'subject_content' THEN
			SET @query = "select *, 
				if(DATE_FORMAT(now(), '%Y-%m-%d')=DATE_FORMAT(delete_date, '%Y-%m-%d'), 
					DATE_FORMAT(delete_date, '%H시 %i분'), DATE_FORMAT(delete_date, '%y-%m-%d')) as post_time from
				Board_Forum where part_id =? and
				(subject like concat('%',?,'%') or content like concat('%',?,'%'))
				and subject is not null and delete_date is not null order by notice desc, reply_group desc, reply_seq asc, group_id desc, post_id desc
				limit ? offset ?";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @search, @search, @limit_id, @offset_id;

		ELSEIF pi_select = 'member_name' THEN
			SET @query = "select *, 
				if(DATE_FORMAT(now(), '%Y-%m-%d')=DATE_FORMAT(delete_date, '%Y-%m-%d'), 
					DATE_FORMAT(delete_date, '%H시 %i분'), DATE_FORMAT(delete_date, '%y-%m-%d')) as post_time from
				Board_Forum where part_id =? and member_name = ?
				and subject is not null and delete_date is not null order by notice desc, reply_group desc, reply_seq asc, group_id desc, post_id desc
				limit ? offset ?";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @search, @limit_id, @offset_id;

		ELSEIF pi_select = 'member_name_comment' THEN
			SET @query = "select *, 
				if(DATE_FORMAT(now(), '%Y-%m-%d')=DATE_FORMAT(delete_date, '%Y-%m-%d'), 
					DATE_FORMAT(delete_date, '%H시 %i분'), DATE_FORMAT(delete_date, '%y-%m-%d')) as post_time from
				Board_Forum where part_id =? and subject is not null and group_id in
				(select group_id from
					Board_Forum where part_id =? and member_name = ?
					and comment_seq > 0 and delete_date is not null)
				limit ? offset ?";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @part_id, @search, @limit_id, @offset_id;
		END IF;
	END IF;
END//
DELIMITER ;


-- 프로시저 bigyo.sp_board_modify_comment 구조 내보내기
DELIMITER //
CREATE DEFINER=`bigyo`@`%` PROCEDURE `sp_board_modify_comment`(
		IN pi_post_id INT,
		IN pi_member_name VARCHAR(50),
		IN pi_content TEXT,
		IN pi_ip VARCHAR(50))
BEGIN
	DECLARE v_part_id INT;
	DECLARE v_cont TEXT;

	select content into v_cont from Board_Forum where post_id = pi_post_id;
	select part_id into v_part_id from Board_Forum where post_id = pi_post_id;

	update Board_Forum set
		member_name = pi_member_name, content = pi_content, ip = pi_ip, modify_count = modify_count+1
		where post_id = pi_post_id;

	insert into Board_Forum_Log(part_id, post_id, before_content, modify_date)
		select v_part_id, pi_post_id, v_cont, now() from Board_Forum where post_id = pi_post_id;

END//
DELIMITER ;


-- 프로시저 bigyo.sp_board_post 구조 내보내기
DELIMITER //
CREATE DEFINER=`bigyo`@`%` PROCEDURE `sp_board_post`(
		IN pi_post_id INT,
		IN pi_part_id INT,
		IN pi_member_num INT,
		IN pi_member_name VARCHAR(30),
		IN pi_subject VARCHAR(50),
		IN pi_content TEXT,
		IN pi_ip VARCHAR(50),
		IN pi_category VARCHAR(20),
		IN pi_attachment INT,
		IN pi_parent_post_id INT,
		IN pi_method VARCHAR(50),
		IN pi_contact VARCHAR(50),
		IN pi_price VARCHAR(50),
		IN pi_reply_group INT,
		IN pi_reply_seq INT,
		IN pi_selected VARCHAR(50))
BEGIN
	DECLARE v_post_point INT;
	DECLARE v_bsub VARCHAR(50);
	DECLARE v_cont TEXT;

	IF pi_post_id > 0 THEN
		select subject into v_bsub from Board_Forum where post_id = pi_post_id;
		select content into v_cont from Board_Forum where post_id = pi_post_id;

		update Board_Forum set
			member_name = pi_member_name, subject = pi_subject, attachment = attachment + pi_attachment,
			content = pi_content, ip = pi_ip, category = pi_category, modify_count = modify_count+1,
			selected = pi_selected, notice = if(pi_category = '공지', 1, 0) where post_id = pi_post_id;

		insert into Board_Forum_Log(part_id, post_id, before_subject, before_content, modify_date)
			select 1, pi_post_id, v_bsub, v_cont, now() from Board_Forum where post_id = pi_post_id;

		update Board_Forum set modify_count = modify_count+1 where post_id = pi_post_id;

		select post_id from Board_Forum where post_id = pi_post_id;

	ELSEIF pi_post_id = 0 OR pi_post_id is null THEN
		select post_point into v_post_point from Manage_Board where board_id = pi_part_id;
		update Members set point = point + v_post_point where num = pi_member_num;

		IF pi_parent_post_id is null OR pi_parent_post_id = 0 THEN
			insert into
				Board_Forum(part_id, member_num, member_name, subject, content, ip, category,
					attachment, method, contact, price, notice, selected,
					post_date, read_date, group_id, reply_group)
				select
					pi_part_id, pi_member_num, pi_member_name, pi_subject, pi_content, pi_ip, pi_category,
					pi_attachment, pi_method, pi_contact, pi_price, if(pi_category = '공지', 1, 0), pi_selected,
					now(), now(), ifnull(max(group_id)+1, 1), ifnull(max(reply_group)+1, 1) from Board_Forum;
		ELSEIF pi_parent_post_id > 0 THEN
			insert into 
				Board_Forum(part_id, member_num, member_name, subject, content, ip, category,
					attachment, method, contact, price, notice, selected,
					post_date, read_date, group_id, reply_group, reply_seq, reply_level)
				select
					pi_part_id, pi_member_num, pi_member_name, pi_subject, pi_content, pi_ip, pi_category,
					pi_attachment, pi_method, pi_contact, pi_price, if(pi_category = '공지', 1, 0), pi_selected,
					now(), now(), ifnull(max(group_id)+1, 1),
					pi_reply_group, reply_seq+1, reply_level+1 from Board_Forum
					where post_id = pi_parent_post_id;
		END IF;
		select LAST_INSERT_ID();
	END IF;
END//
DELIMITER ;


-- 프로시저 bigyo.sp_board_read 구조 내보내기
DELIMITER //
CREATE DEFINER=`bigyo`@`%` PROCEDURE `sp_board_read`(
		IN pi_member_num INT,
		IN pi_post_id INT)
BEGIN
	DECLARE v_post_member INT;
	DECLARE v_part_id INT;
	DECLARE v_read_point INT;
	select member_num into v_post_member from Board_Forum where post_id = pi_post_id;
	select part_id into v_part_id from Board_Forum where post_id = pi_post_id;

	IF pi_member_num = v_post_member THEN
		update Board_Forum set read_date = now() where post_id = pi_post_id;
		
	ELSEIF pi_member_num > 0 THEN
		select read_point into v_read_point from Manage_Board where board_id = v_part_id;
		update Members set point = point + v_read_point where num = pi_member_num;
	END IF;

	update Board_Forum set count = count +1 where post_id = pi_post_id;

	select *, Members.picture as member_pic, Members.signature as signature,
		DATE_FORMAT(post_date, '%Y년 %m월 %d일 %H시 %i분') as post_fulltime
		from Board_Forum inner join Members
		on Board_Forum.member_num = Members.num
		where post_id = pi_post_id and delete_date is null;
END//
DELIMITER ;


-- 프로시저 bigyo.sp_board_total 구조 내보내기
DELIMITER //
CREATE DEFINER=`bigyo`@`%` PROCEDURE `sp_board_total`(
		IN pi_part_id INT,
		IN pi_category VARCHAR(20),
		IN pi_select VARCHAR(20),
		IN pi_search VARCHAR(100))
BEGIN

	SET @part_id = pi_part_id;
	SET @category = pi_category;
	SET @select = pi_select;
	SET @search = pi_search;
	
	IF pi_category is null AND pi_search is null THEN
		SET @query = "select count(*) as total_record from 
			Board_Forum where part_id = ? and subject is not null and delete_date is null";
		PREPARE stmt FROM @query;
		EXECUTE stmt USING @part_id;

	ELSEIF pi_category is not null AND pi_search is null THEN
		SET @query = "select count(*) as total_record from 
			Board_Forum where part_id =? and category =? 
			and subject is not null and delete_date is null";
		PREPARE stmt FROM @query;
		EXECUTE stmt USING @part_id, @category;

	ELSEIF pi_search is not null THEN
		IF pi_select = 'subject' THEN
			SET @query = "select count(*) as total_record from 
				Board_Forum where part_id =? and subject like concat('%',?,'%')
				and subject is not null and delete_date is null";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @search;

		ELSEIF pi_select = 'content' THEN
			SET @query = "select count(*) as total_record from 
				Board_Forum where part_id =? and content like concat('%',?,'%')
				and subject is not null and delete_date is null";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @search;

		ELSEIF pi_select = 'comment' THEN
			SET @query = "select count(*) as total_record from 
				Board_Forum where part_id =? and subject is not null and group_id in
				(select group_id from
					Board_Forum where part_id=? content like concat('%',?,'%')
					and comment_seq > 0 and delete_date is null)";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @part_id, @search;

		ELSEIF pi_select = 'subject_content' THEN
			SET @query = "select count(*) as total_record from 
				Board_Forum where part_id =? and
				(subject like concat('%',?,'%') or content like concat('%',?,'%'))
				and subject is not null and delete_date is null";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @search, @search;

		ELSEIF pi_select = 'member_name' THEN
			SET @query = "select count(*) as total_record from 
				Board_Forum where part_id =? and member_name = ?
				and subject is not null and delete_date is null";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @search;

		ELSEIF pi_select = 'member_name_comment' THEN
			SET @query = "select count(*) as total_record from 
				Board_Forum where part_id =? and subject is not null and group_id in
				(select group_id from
					Board_Forum where part_id =? and member_name = ?
					and comment_seq > 0 and delete_date is null)";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @part_id, @search;
		END IF;
	END IF;
END//
DELIMITER ;


-- 프로시저 bigyo.sp_board_total_del 구조 내보내기
DELIMITER //
CREATE DEFINER=`bigyo`@`%` PROCEDURE `sp_board_total_del`(
		IN pi_part_id INT,
		IN pi_category VARCHAR(20),
		IN pi_select VARCHAR(20),
		IN pi_search VARCHAR(100))
BEGIN

	SET @part_id = pi_part_id;
	SET @category = pi_category;
	SET @select = pi_select;
	SET @search = pi_search;
	
	IF pi_category is null AND pi_search is null THEN
		SET @query = "select count(*) as total_record from 
			Board_Forum where part_id = ? and subject is not null and delete_date is not null";
		PREPARE stmt FROM @query;
		EXECUTE stmt USING @part_id;

	ELSEIF pi_category is not null AND pi_search is null THEN
		SET @query = "select count(*) as total_record from 
			Board_Forum where part_id =? and category =? 
			and subject is not null and delete_date is not null";
		PREPARE stmt FROM @query;
		EXECUTE stmt USING @part_id, @category;

	ELSEIF pi_search is not null THEN
		IF pi_select = 'subject' THEN
			SET @query = "select count(*) as total_record from 
				Board_Forum where part_id =? and subject like concat('%',?,'%')
				and subject is not null and delete_date is not null";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @search;

		ELSEIF pi_select = 'content' THEN
			SET @query = "select count(*) as total_record from 
				Board_Forum where part_id =? and content like concat('%',?,'%')
				and subject is not null and delete_date is not null";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @search;

		ELSEIF pi_select = 'comment' THEN
			SET @query = "select count(*) as total_record from 
				Board_Forum where part_id =? and subject is not null and group_id in
				(select group_id from
					Board_Forum where part_id=? content like concat('%',?,'%')
					and comment_seq > 0 and delete_date is not null)";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @part_id, @search;

		ELSEIF pi_select = 'subject_content' THEN
			SET @query = "select count(*) as total_record from 
				Board_Forum where part_id =? and
				(subject like concat('%',?,'%') or content like concat('%',?,'%'))
				and subject is not null and delete_date is not null";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @search, @search;

		ELSEIF pi_select = 'member_name' THEN
			SET @query = "select count(*) as total_record from 
				Board_Forum where part_id =? and member_name = ?
				and subject is not null and delete_date is not null";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @search;

		ELSEIF pi_select = 'member_name_comment' THEN
			SET @query = "select count(*) as total_record from 
				Board_Forum where part_id =? and subject is not null and group_id in
				(select group_id from
					Board_Forum where part_id =? and member_name = ?
					and comment_seq > 0 and delete_date is not null)";
			PREPARE stmt FROM @query;
			EXECUTE stmt USING @part_id, @part_id, @search;
		END IF;
	END IF;
END//
DELIMITER ;


-- 프로시저 bigyo.sp_likes_rating 구조 내보내기
DELIMITER //
CREATE DEFINER=`bigyo`@`%` PROCEDURE `sp_likes_rating`(
		IN pi_member_num INT,
		IN pi_post_id INT,
		IN pi_like INT)
BEGIN
	DECLARE v_point INT;
	DECLARE v_part_id INT;
	DECLARE v_like_count INT;
	DECLARE v_member_level INT;
	DECLARE v_birth INT;
	DECLARE v_gender INT;

	select part_id into v_part_id from Board_Forum where post_id = pi_post_id;
	select level into v_member_level from Members where num = pi_member_num;
	select birth into v_birth from Members where num = pi_member_num;
	select gender into v_gender from Members where num = pi_member_num;
	select count(like_list) into v_like_count from Like_Avg where post_id = pi_post_id and like_list like concat('%#', pi_member_num, '$%');

	IF v_like_count = 0 THEN
		select like_point into v_point from Manage_Board where board_id = v_part_id;
		update Board_Forum set likes = likes + pi_like, dislike = dislike+1 where post_id = pi_post_id;
		update Members set point = point + v_point where num = pi_member_num;
		update Like_Avg set likes = likes + pi_like, dislike = dislike+1, like_list = concat(ifnull(like_list, 0), '#', pi_member_num, '$') where post_id = pi_post_id;
		update Like_Avg set age_like = age_like + (DATE_FORMAT(now(), '%Y')-v_birth) where post_id = pi_post_id;
			
		IF v_gender = 'male' THEN
			update Like_Avg set male_like = male_like+pi_like, male_dislike = male_dislike+1 where post_id = pi_post_id;
		ELSEIF v_gender = 'female' THEN
			update Like_Avg set female_like = female_like+pi_like, female_like = female_dislike+1 where post_id = pi_post_id;
		end if;
	end if;
	select ROUND(likes/dislike, 1) as likes from Board_Forum where post_id = pi_post_id;
END//
DELIMITER ;


-- 프로시저 bigyo.sp_likes_value 구조 내보내기
DELIMITER //
CREATE DEFINER=`bigyo`@`%` PROCEDURE `sp_likes_value`(
		IN pi_member_num INT,
		IN pi_post_id INT,
		IN pi_like VARCHAR(20))
BEGIN
	DECLARE v_point INT;
	DECLARE v_part_id INT;
	DECLARE v_like_count INT;
	DECLARE v_member_level INT;
	DECLARE v_birth INT;
	DECLARE v_gender INT;

	select part_id into v_part_id from Board_Forum where post_id = pi_post_id;
	select level into v_member_level from Members where num = pi_member_num;
	select birth into v_birth from Members where num = pi_member_num;
	select gender into v_gender from Members where num = pi_member_num;
	select count(like_list) into v_like_count from Like_Avg where post_id = pi_post_id and like_list like concat('%#', pi_member_num, '$%');

	IF v_like_count = 0 THEN
		IF pi_like = 'like' THEN
			select like_point into v_point from Manage_Board where board_id = v_part_id;
			update Board_Forum set likes = likes+1 where post_id = pi_post_id;
			update Members set point = point + v_point where num = pi_member_num;
			update Like_Avg set likes = likes+1, like_list = concat(ifnull(like_list, 0), '#', pi_member_num, '$') where post_id = pi_post_id;
			update Like_Avg set age_like = age_like + (DATE_FORMAT(now(), '%Y')-v_birth) where post_id = pi_post_id;
			
			IF v_gender = 'male' THEN
				update Like_Avg set male_like = male_like+1 where post_id = pi_post_id;
			ELSEIF v_gender = 'female' THEN
				update Like_Avg set female_like = female_like+1 where post_id = pi_post_id;
			END IF;

		ELSEIF pi_like = 'dislike' THEN
			select like_point into v_point from Manage_Board where board_id = v_part_id;
			update Board_Forum set dislike = dislike+1 where post_id = pi_post_id;
			update Members set point = point + v_point where num = pi_member_num;
			update Like_Avg set dislike = dislike+1, like_list = concat(ifnull(like_list, 0), '#', pi_member_num, '$') where post_id = pi_post_id;
			update Like_Avg set age_dislike = age_dislike + (DATE_FORMAT(now(), '%Y')-v_birth) where post_id = pi_post_id;
			
			IF v_gender = 'male' THEN
				update Like_Avg set male_dislike = male_dislike+1 where post_id = pi_post_id;
			ELSEIF v_gender = 'female' THEN
				update Like_Avg set female_dislike = female_dislike+1 where post_id = pi_post_id;
			END IF;
		end if;
	end if;
	select concat(likes, '#', dislike) as likes from Board_Forum where post_id = pi_post_id;
END//
DELIMITER ;


-- 프로시저 bigyo.sp_make_jot 구조 내보내기
DELIMITER //
CREATE DEFINER=`bigyo`@`%` PROCEDURE `sp_make_jot`(
		IN pi_pigo INT,
		IN pi_judgement INT,
		IN pi_num INT)
BEGIN
	DECLARE v_date DATETIME;

	select prison_break into v_date from Members where num = pi_pigo;
	update Members_Singo set judgement = pi_judgement where num = pi_num;

	IF pi_judgement = 2 THEN
		insert into Message(send_member, receive_member, send_date, messagecontent)
		values(0, pi_num, now(), '귀하는 본 사이트의 규정을 위반하였으나, 사안이 경미하고 고의성이 없다고 판단되어 처벌하지 않았습니다. 

앞으로 주의하여 주십시오.');

	ELSEIF pi_judgement = 99 THEN
		IF v_date > now() THEN
			update Members set prison_break = date_add(prison_break, interval +99 year) where num = pi_pigo;
		ELSE
			update Members set prison_break = date_add(now(), interval +99 year) where num = pi_pigo;

		END IF;

		insert into Message(send_member, receive_member, send_date, messagecontent)
		values(0, pi_pigo, now(), concat('귀하는 본 사이트의 규정을 심각하게 위반하였으므로, 영구유배 조치를 내립니다.'));

	ELSEIF pi_judgement != 1 THEN
		IF v_date > now() THEN
			update Members set prison_break = date_add(prison_break, interval +pi_judgement day) where num = pi_pigo;
		ELSE
			update Members set prison_break = date_add(now(), interval +pi_judgement day) where num = pi_pigo;

		END IF;

		insert into Message(send_member, receive_member, send_date, messagecontent)
		values(0, pi_pigo, now(), concat('귀하는 본 사이트의 규정을 위반하였으므로, ', pi_judgement,'일 간의 유배형을 내립니다.'));

	END IF;
END//
DELIMITER ;


-- 뷰 bigyo.view_message 구조 내보내기
-- VIEW 종속성 오류를 극복하기 위해 임시 테이블을 생성합니다.
CREATE TABLE `view_message` (
	`Num` INT(11) NOT NULL COMMENT '쪽지번호',
	`Send_Member` INT(11) NULL COMMENT '보낸사람',
	`Receive_Member` INT(11) NULL COMMENT '받는사람',
	`Send_Date` DATETIME NULL COMMENT '보낸시간',
	`Read_Date` DATETIME NULL COMMENT '읽은시간',
	`Delete_Check` INT(1) NULL COMMENT '삭제체크',
	`MessageContent` VARCHAR(1000) NULL COLLATE 'utf8_general_ci',
	`send_member_name` VARCHAR(50) NOT NULL COMMENT '별명' COLLATE 'utf8_general_ci',
	`receive_member_name` VARCHAR(50) NOT NULL COMMENT '별명' COLLATE 'utf8_general_ci'
) ENGINE=MyISAM;


-- 뷰 bigyo.view_singo_chuli 구조 내보내기
-- VIEW 종속성 오류를 극복하기 위해 임시 테이블을 생성합니다.
CREATE TABLE `view_singo_chuli` (
	`num` INT(11) NOT NULL COMMENT '신고번호',
	`Post_ID` INT(11) NOT NULL COMMENT '신고글',
	`Wongo` INT(11) NULL COMMENT '신고자',
	`Pigo` INT(11) NULL COMMENT '신고대상',
	`Sayu` VARCHAR(100) NULL COMMENT '신고사유' COLLATE 'utf8_general_ci',
	`Memo` VARCHAR(500) NULL COMMENT '비고' COLLATE 'utf8_general_ci',
	`Singo_Date` DATETIME NULL COMMENT '신고일자',
	`Judgement` INT(1) NOT NULL COMMENT '처리여부',
	`wongo_name` VARCHAR(50) NOT NULL COMMENT '별명' COLLATE 'utf8_general_ci',
	`pigo_name` VARCHAR(50) NOT NULL COMMENT '별명' COLLATE 'utf8_general_ci'
) ENGINE=MyISAM;


-- 뷰 bigyo.view_singo_list 구조 내보내기
-- VIEW 종속성 오류를 극복하기 위해 임시 테이블을 생성합니다.
CREATE TABLE `view_singo_list` (
	`num` INT(11) NOT NULL COMMENT '신고번호',
	`Post_ID` INT(11) NOT NULL COMMENT '신고글',
	`Wongo` INT(11) NULL COMMENT '신고자',
	`Pigo` INT(11) NULL COMMENT '신고대상',
	`Sayu` VARCHAR(100) NULL COMMENT '신고사유' COLLATE 'utf8_general_ci',
	`Memo` VARCHAR(500) NULL COMMENT '비고' COLLATE 'utf8_general_ci',
	`Singo_Date` DATETIME NULL COMMENT '신고일자',
	`Judgement` INT(1) NOT NULL COMMENT '처리여부',
	`wongo_name` VARCHAR(50) NOT NULL COMMENT '별명' COLLATE 'utf8_general_ci',
	`pigo_name` VARCHAR(50) NOT NULL COMMENT '별명' COLLATE 'utf8_general_ci'
) ENGINE=MyISAM;


-- 뷰 bigyo.view_message 구조 내보내기
-- 임시 테이블을 제거하고 최종 VIEW 구조를 생성
DROP TABLE IF EXISTS `view_message`;
CREATE ALGORITHM=UNDEFINED DEFINER=`bigyo`@`%` SQL SECURITY DEFINER VIEW `view_message` AS select `Message`.`Num` AS `Num`,`Message`.`Send_Member` AS `Send_Member`,`Message`.`Receive_Member` AS `Receive_Member`,`Message`.`Send_Date` AS `Send_Date`,`Message`.`Read_Date` AS `Read_Date`,`Message`.`Delete_Check` AS `Delete_Check`,`Message`.`MessageContent` AS `MessageContent`,`m1`.`Name` AS `send_member_name`,`m2`.`Name` AS `receive_member_name` from ((`Message` join `Members` `m1` on((`Message`.`Send_Member` = `m1`.`Num`))) join `Members` `m2` on((`Message`.`Receive_Member` = `m2`.`Num`))) order by `Message`.`Send_Date`;


-- 뷰 bigyo.view_singo_chuli 구조 내보내기
-- 임시 테이블을 제거하고 최종 VIEW 구조를 생성
DROP TABLE IF EXISTS `view_singo_chuli`;
CREATE ALGORITHM=UNDEFINED DEFINER=`bigyo`@`%` SQL SECURITY DEFINER VIEW `view_singo_chuli` AS select `Members_Singo`.`num` AS `num`,`Members_Singo`.`Post_ID` AS `Post_ID`,`Members_Singo`.`Wongo` AS `Wongo`,`Members_Singo`.`Pigo` AS `Pigo`,`Members_Singo`.`Sayu` AS `Sayu`,`Members_Singo`.`Memo` AS `Memo`,`Members_Singo`.`Singo_Date` AS `Singo_Date`,`Members_Singo`.`Judgement` AS `Judgement`,`m1`.`Name` AS `wongo_name`,`m2`.`Name` AS `pigo_name` from ((`Members_Singo` join `Members` `m1` on((`Members_Singo`.`Wongo` = `m1`.`Num`))) join `Members` `m2` on((`Members_Singo`.`Pigo` = `m2`.`Num`))) where (`Members_Singo`.`Judgement` > 0) order by `Members_Singo`.`Singo_Date`;


-- 뷰 bigyo.view_singo_list 구조 내보내기
-- 임시 테이블을 제거하고 최종 VIEW 구조를 생성
DROP TABLE IF EXISTS `view_singo_list`;
CREATE ALGORITHM=UNDEFINED DEFINER=`bigyo`@`%` SQL SECURITY DEFINER VIEW `view_singo_list` AS select `Members_Singo`.`num` AS `num`,`Members_Singo`.`Post_ID` AS `Post_ID`,`Members_Singo`.`Wongo` AS `Wongo`,`Members_Singo`.`Pigo` AS `Pigo`,`Members_Singo`.`Sayu` AS `Sayu`,`Members_Singo`.`Memo` AS `Memo`,`Members_Singo`.`Singo_Date` AS `Singo_Date`,`Members_Singo`.`Judgement` AS `Judgement`,`m1`.`Name` AS `wongo_name`,`m2`.`Name` AS `pigo_name` from ((`Members_Singo` join `Members` `m1` on((`Members_Singo`.`Wongo` = `m1`.`Num`))) join `Members` `m2` on((`Members_Singo`.`Pigo` = `m2`.`Num`))) order by `Members_Singo`.`Singo_Date`;
