-- 테이블 bigyo.Sample_Board_Log 구조 내보내기
CREATE TABLE IF NOT EXISTS `Sample_Board_Log` (
  `Log_ID` int(11) NOT NULL auto_increment COMMENT '고유번호',
  `Post_ID` int(11) default NULL COMMENT '글번호',
  `Before_Subject` varchar(50) default NULL COMMENT '제목',
  `Before_Content` varchar(5000) default NULL COMMENT '내용',
  `Modify_Date` datetime default NULL COMMENT '수정일자',
  PRIMARY KEY  (`Log_ID`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COMMENT='게시물 수정시 로그';

