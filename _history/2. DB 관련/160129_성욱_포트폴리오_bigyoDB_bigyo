-- 테이블 bigyo.Board_Market_Attachment 구조 내보내기
CREATE TABLE IF NOT EXISTS `Board_Market_Attachment` (
  `Attach_Num` int(11) NOT NULL auto_increment COMMENT '첨부번호',
  `Post_Num` int(11) default NULL COMMENT '글번호',
  `Member_Num` int(11) default NULL COMMENT '회원번호',
  `File_Name` varchar(1000) default NULL COMMENT '퍄앨이름',
  `Download_Count` int(11) default NULL COMMENT '다운받은 횟수',
  `Upload_Date` datetime default NULL COMMENT '업로드한 날짜',
  PRIMARY KEY  (`Attach_Num`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC COMMENT='첨부파일';

